@model IEnumerable<L03_HomeWorkB.Models.Product>

<h4>Table List</h4>

<table border="1">
    <tr>
        <th>Id</th>
        <th>Name</th>
        <th>Price</th>
        <th>Created Date</th>
    </tr>
   
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.Id</td>
            <td>@item.Name</td>
            <td>@item.Price</td>
            <td>@item.CreatedDate.ToShortDateString()</td>
        </tr>
    }
</table>