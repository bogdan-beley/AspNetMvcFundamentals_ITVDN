
@model List<L04_HomeWorkB.Models.Login>

@{
    ViewBag.Title = "Login";
}

<h2>Login</h2>

@using (Html.BeginForm("Login", "Autorization"))
{
    <label>Логін:</label>
    @Html.TextBox("username")<br/>
    <label>Пароль:</label>
    @Html.TextBox("password")<br />

    <input type="submit" name="loginform" value="Login">
}


